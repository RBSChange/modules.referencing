<?xml version="1.0" encoding="utf-8"?>
<layout xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul" xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul" xmlns:i18n="http://phptal.motion-twin.com/i18n"
	xmlns:tal="http://phptal.motion-twin.com/tal" xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal">
	<tabbox flex="1">
		<tabs>
			<tab anonid="builder_auto-generated-tab" i18n:attributes="label &amp;modules.generic.bo.general.GeneralitiesSpaced;" change:icon="information/small shadow" />
		</tabs>
		<tabpanels flex="1">
			<tabpanel anonid="builder_auto-generated-tabpanel" orient="vertical">
				<grid flex="1">
					<columns>
						<column />
						<column flex="1" />
					</columns>
					<rows>
						<row anonid="package-row">
							<change:label xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" field="package" />
							<change:field xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" name="package" />
						</row>
						<row anonid="baseFileSignature-row">
							<change:label xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" field="baseFileSignature" />
							<change:field xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" name="baseFileSignature" />
						</row>
						<row>
							<change:label xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" field="content" />
							<vbox>
								<change:field xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" name="content" />
								<hbox>
									<button change:i18nattr="label &amp;modules.referencing.bo.general.Reload-default-rulesSpaced;" change:icon="refresh/small" oncommand="reloadDefaultRules();" />
									<spacer flex="1" />
								</hbox>
							</vbox>
						</row>
						<row>
							<change:label xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" field="commentary" />
							<change:field xmlns:change="http://www.rbs-change.eu/change-4.2/taglib/phptal" name="commentary" />
						</row>
					</rows>
				</grid>
			</tabpanel>
		</tabpanels>
	</tabbox>
</layout>